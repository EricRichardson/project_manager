<h1>Log in</h1>
<%= bootstrap_form_tag url: sessions_path do |f| %>
    <%= f.text_field :email %>

    <%= f.password_field :password %>

    <%= link_to "Forgot password?", new_password_reset_path %>
    <%= link_to "Sign in with Github", sign_in_with_github_path %>
    <%= f.submit "Login" %>
<% end %>
