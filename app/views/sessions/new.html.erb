<h1>Log in</h1>
<%= form_tag sessions_path do %>
  <div>
    <%= label_tag :email %>
    <%= text_field_tag :email %>
  </div>
  <div>
    <%= label_tag :password %>
    <%= password_field_tag :password %>
  </div>
  <%= link_to "Forgot password?", new_password_reset_path %>
  <%= submit_tag "Login" %>
<% end %>
