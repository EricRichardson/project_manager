<%= bootstrap_form_for @project do |f| %>
    <%= f.text_field :title %>

    <%= f.text_field :description %>

    <%= f.collection_check_boxes(:tag_ids, Tag.order(:title), :id, :title)  %>

    <%= f.collection_check_boxes(:user_ids, User.all, :id, :full_name) %>
    <%= f.date_select :due_date %>

    <%= f.submit %>
<% end %>
